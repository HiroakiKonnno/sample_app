<h1>記事新規作成</h1>
<%= form_with(model: @article, url:user_article_index_path, method: :post ,local: true) do |f| %>
  <h5>タイトル</h5>
  <%= f.text_field :title, class: 'form-control' %>
  <h5>本文</h5>
  <%= f.text_field :content, class: 'form-control' %>
  <h5>画像</h5>
  <div class="field">
    <%= f.file_field :image %>
  </div>
  <%= f.hidden_field :user_id, :value => @user.id %>
  <%= f.submit "作成", class: "btn btn-primary btn-block btn-signup" %>
<% end %>